{
	"title": "Example Schema",
	"type": "object",
	"properties": {
		"name": {
			"type": "string"
		},
		"images": {
			"type": "object",
			"properties": {
				"schematic": {"type": "string"},
				"thumbnail": {"type": "string"}
			}
		},
		"price": {
			"type": "number"
		},
    "insurance": {
			"type": "number"
		},
    "dimensions": {
        "type": "object",
        "properties": {
            "length": {"type": "number"},
            "width": {"type": "number"},
            "height": {"type": "number"}
        },
        "required": ["length", "width", "height"]
    },
    "speed": {
        "type": "object",
        "properties": {
            "top": {"type": "number"},
            "boost": {"type": "number"},
        },
        "required": ["top", "boost"]
    }
	},
  "required": ["name", "price"]
}
